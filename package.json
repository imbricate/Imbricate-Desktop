{
  "name": "@imbricate/desktop",
  "packageManager": "yarn@4.5.1",
  "version": "1.0.0",
  "description": "The Desktop Application for Imbricate",
  "scripts": {
    "clean": "rm -rf app",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "build": "tsc --project typescript/tsconfig.build.json",
    "license": "sdb license",
    "release": "yarn lint && yarn test && yarn license && yarn build",
    "publish-dry-run": "yarn release && sdb --only publish-dry-run",
    "publish": "yarn release && sdb --only publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Imbricate/Imbricate-Desktop.git"
  },
  "keywords": [
    "Notebook",
    "Engineer",
    "API"
  ],
  "author": "WMXPY",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Imbricate/Imbricate-Desktop/issues"
  },
  "homepage": "https://imbricate.io",
  "dependencies": {
    "electron": "^33.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "babel-jest": "^29.7.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
